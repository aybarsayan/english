generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Parent {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  email         String   @unique
  password      String
  name          String
  phone         String   @unique
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
